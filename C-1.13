def myReverse(data):
    # returning a new reversed list
    return [data[i] for i in range(len(data)-1, -1, -1)]


def myReverseGen(data):
    # returning a reversed generator
    for i in range(len(data)-1, -1, -1):
        yield data[i]


def myReverseGenWithComprehension(data):
    # returning a reversed generator
    return (data[i] for i in range(len(data)-1, -1, -1))